                '';
            flowchart.parse(code1).drawSVG('canvas1', {});

            // canvas2
            var code2 =
                'st=>start: 区域体验总店\n' +
                'e=>end: 消费者\n' +
                'io1=>inputoutput: 网店/PC商城\n' +
                'io2=>inputoutput: 导购达人/微商\n' +
                '' +
                'st(right)->io1(right)->io2(right)->e\n' +
                '';
            flowchart.parse(code2).drawSVG('canvas2', {});

            // canvas3
            var code3 =
                'st=>start: 加盟店/合作外店\n' +
                'e=>end: 消费者\n' +
                'io1=>inputoutput: 导购达人/微商\n' +
                '' +
                'st(right)->io1(right)->e\n' +
